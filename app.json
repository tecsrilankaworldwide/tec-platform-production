{
  "name": "TEC Learning Platform",
  "description": "A comprehensive learning and training platform built with React and FastAPI",
  "repository": "https://github.com/tecsrilankaworldwide/tec-platform-production",
  "keywords": ["learning-platform", "education", "react", "fastapi", "mongodb"],
  "env": {
    "MONGO_URL": {
      "description": "MongoDB Atlas connection string",
      "required": true
    },
    "DB_NAME": {
      "description": "Database name",
      "value": "tec_platform"
    },
    "SECRET_KEY": {
      "description": "JWT secret key",
      "generator": "secret"
    },
    "CORS_ORIGINS": {
      "description": "CORS origins",
      "value": "*"
    }
  }
}